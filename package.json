  "license": "MIT",
  "scripts": {
    "serve": "cross-env CARRIER_VERSION=$(git tag --sort=version:refname | head -n 1) NODE_ENV=development webpack serve",
    "build": "npm -v && npm run clean && CARRIER_VERSION=$(git tag --sort=version:refname | head -n 1) NODE_ENV=development webpack",
    "build:production": "npm -v && npm run clean && NODE_ENV=production webpack",
    "clean": "rm -rf dist",
    "generate-indexer-client": "rm -rf src/indexer-client && openapi-generator-cli generate -c openapi.json -i src/assets/openapi-spec.json -g typescript-fetch -o src/indexer-client"
  },
